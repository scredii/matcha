CREATE TABLE hashtag ( id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, hashtag VARCHAR(255) NOT NULL, content_id int(30) NOT NULL);
CREATE TABLE pictures ( id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, picture VARCHAR(255) NOT NULL, content_id int(30) NOT NULL, pp INT(6) NULL);
CREATE TABLE locations ( id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY, latitude FLOAT(20) NOT NULL, longitude FLOAT(20) NOT NULL, city VARCHAR(255) NOT NULL, id_content int(30) NOT NULL);




locations SET latitude = ?, longitude = ?, city = ?, id_content = ?